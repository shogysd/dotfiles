[
  // カーソルの移動
  {
    "key": "ctrl+p",
    "command": "cursorUp",
    "when": "!terminalFocus"
  },
  {
    "key": "ctrl+n",
    "command": "cursorDown",
    "when": "!terminalFocus"
  },
  {
    "key": "ctrl+f",
    "command": "cursorRight",
    "when": "textInputFocus"
  },
  {
    "key": "PageDown",
    "command": "cursorRight",
    "when": "textInputFocus"
  },
  {
    "key": "ctrl+b",
    "command": "cursorLeft",
    "when": "textInputFocus"
  },
  {
    "key": "PageUp",
    "command": "cursorLeft",
    "when": "textInputFocus"
  },
  // 入力補完
  {
    "key": "tab",
    "command": "selectNextSuggestion",
    "when": "editorTextFocus && suggestWidgetMultipleSuggestions && suggestWidgetVisible"
  },
  {
    "key": "ctrl+n",
    "command": "selectNextSuggestion",
    "when": "editorTextFocus && suggestWidgetMultipleSuggestions && suggestWidgetVisible"
  },
  {
    "key": "shift+tab",
    "command": "selectPrevSuggestion",
    "when": "editorTextFocus && suggestWidgetMultipleSuggestions && suggestWidgetVisible"
  },
  {
    "key": "ctrl+p",
    "command": "selectPrevSuggestion",
    "when": "editorTextFocus && suggestWidgetMultipleSuggestions && suggestWidgetVisible"
  },
  {
    "key": "Escape",
    "command": "hideSuggestWidget",
    "when": "editorTextFocus && suggestWidgetMultipleSuggestions && suggestWidgetVisible"
  },
  // カーソルの移動 (行単位)
  {
    "key": "ctrl+a",
    "command": "cursorHome",
    "when": "textInputFocus"
  },
  {
    "key": "ctrl+e",
    "command": "cursorEnd",
    "when": "textInputFocus"
  },
  // カーソルの移動 (ファイル単位)
  //// ファイルの先頭
  {
    "key": "alt+up",
    "command": "cursorTop",
    "when": "textInputFocus"
  },
  //// ファイルの末尾
  {
    "key": "alt+down",
    "command": "cursorBottom",
    "when": "textInputFocus"
  },
  // マルチカーソル
  {
    "key": "ctrl+shift+p",
    "command": "editor.action.insertCursorAbove",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+shift+up",
    "command": "editor.action.insertCursorAbove",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+shift+n",
    "command": "editor.action.insertCursorBelow",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+shift+down",
    "command": "editor.action.insertCursorBelow",
    "when": "editorTextFocus"
  },
  // 文字の削除
  {
    "key": "ctrl+d",
    "command": "deleteRight",
    "when": "textInputFocus"
  },
  {
    "key": "ctrl+h",
    "command": "deleteLeft",
    "when": "textInputFocus"
  },
  // 文字の削除 (行単位)
  {
    "key": "ctrl+k",
    "command": "deleteAllRight",
    "when": "textInputFocus"
  },
  //{
  //  "key": "ctrl+shift+k",
  //  "command": "deleteAllLeft",
  //  "when": "textInputFocus"
  //},
  // 選択
  {
    "key": "ctrl+alt+p",
    "command": "cursorUpSelect",
    "when": "textInputFocus"
  },
  {
    "key": "ctrl+alt+n",
    "command": "cursorDownSelect",
    "when": "textInputFocus"
  },
  {
    "key": "ctrl+alt+f",
    "command": "cursorRightSelect",
    "when": "textInputFocus"
  },
  {
    "key": "ctrl+alt+b",
    "command": "cursorLeftSelect",
    "when": "textInputFocus"
  },
  // 選択 (全選択)
  {
    "key": "ctrl+shift+a",
    "command": "editor.action.selectAll",
    "when": "textInputFocus"
  },
  {
    "key": "cmd+a",
    "command": "editor.action.selectAll",
    "when": "textInputFocus"
  },
  // 検索・置換
  {
    "key": "ctrl+shift+f",
    "command": "actions.find",
    "when": "editorForcus || editorIsOpen"
  },
  {
    "key": "cmd+f",
    "command": "actions.find",
    "when": "editorForcus || editorIsOpen"
  },
  {
    "key": "ctrl+shift+h",
    "command": "editor.action.startFindReplaceAction",
    "when": "editorForcus || editorIsOpen"
  },
  {
    "key": "cmd+h",
    "command": "editor.action.startFindReplaceAction",
    "when": "editorForcus || editorIsOpen"
  },
  {
    "key": "alt+h",
    "command": "editor.action.addSelectionToNextFindMatch",
    "when": "editorFocus"
  },
  {
    "key": "cmd+d",
    "command": "-editor.action.addSelectionToNextFindMatch",
    "when": "editorFocus"
  },
  // copy, paste, cut
  {
    "key": "ctrl+c",
    "command": "editor.action.clipboardCopyAction",
    "when": "textInputFocus"
  },
  {
    "key": "cmd+c",
    "command": "editor.action.clipboardCopyAction",
    "when": "textInputFocus"
  },
  {
    "key": "ctrl+v",
    "command": "editor.action.clipboardPasteAction",
    "when": "textInputFocus"
  },
  {
    "key": "cmd+v",
    "command": "editor.action.clipboardPasteAction",
    "when": "textInputFocus"
  },
  // redo, undo
  {
    "key": "ctrl+y",
    "command": "redo",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+shift+z",
    "command": "redo",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+shift+z",
    "command": "redo",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+z",
    "command": "undo",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+z",
    "command": "undo",
    "when": "editorTextFocus"
  },
  // 整形
  {
    "key": "ctrl+]",
    "command": "editor.action.format",
    "when": "editorTextFocus"
  },
  // 保存
  {
    "key": "ctrl+s",
    "command": "workbench.action.files.save",
    "when": "!terminalFocus"
  },
  {
    "key": "ctrl+x ctrl+s",
    "command": "workbench.action.files.save",
    "when": "!terminalFocus"
  },
  // editor tab の移動
  {
    "key": "ctrl+right",
    "command": "workbench.action.nextEditor"
    // "when": "!terminalFocus"
  },
  {
    "key": "ctrl+left",
    "command": "workbench.action.previousEditor"
    // "when": "!terminalFocus"
  },
  // エディタを閉じる
  {
    "key": "ctrl+w",
    "command": "workbench.action.closeActiveEditor"
  },
  {
    "key": "ctrl+x ctrl+c",
    "command": "workbench.action.closeActiveEditor"
  },
  // ウィンドウを閉じる
  //{
  //  "key": "ctrl+w",
  //  "command": "workbench.action.closeWindow",
  //  "when": "!editorIsOpen && !multipleEditorGroups" // && !terminalFocus"
  //},
  //{
  //  "key": "ctrl+x ctrl+c",
  //  "command": "workbench.action.closeWindow",
  //  "when": "!editorIsOpen && !multipleEditorGroups" // && !terminalFocus"
  //},
  //ターミナルを開く
  {
    "key": "cmd+t",
    "command": "workbench.action.terminal.new"
  },
  {
    "key": "ctrl+`",
    "command": "workbench.action.togglePanel"
  },
  //{
  //  "key": "ctrl+shift+`",
  //  "command": "workbench.action.createTerminalEditor"
  //},
  // editor group
  //// 分割
  {
    "key": "ctrl+\\",
    "command": "workbench.action.splitEditor"
  },
  //// 分割
  {
    "key": "cmd+\\",
    "command": "workbench.action.splitEditor"
  },
  //// 左の editor group に移動
  {
    "key": "ctrl+shift+left",
    "command": "workbench.action.focusLeftGroup"
  },
  //// 左の editor group に移動
  {
    "key": "cmd+k left",
    "command": "-workbench.action.focusLeftGroup"
  },
  //// 右の editor group に移動
  {
    "key": "ctrl+shift+right",
    "command": "workbench.action.focusRightGroup"
  },
  //// 右の editor group に移動
  {
    "key": "cmd+k right",
    "command": "-workbench.action.focusRightGroup"
  },
  // editorTextFocus -> terminal
  {
    "key": "ctrl+shift+\\",
    "command": "workbench.action.terminal.focus",
    "when": "editorTextFocus"
  },
  // cmd + w で vs-code の終了はしない
  {
    "key": "cmd+w",
    "command": "-workbench.action.closeWindow",
    "when": "!editorIsOpen && !multipleEditorGroups"
  },
  // plugin
  {
    "key": "alt+t",
    "command": "insertDateString.insertOwnFormatDateTime",
    "when": "editorTextFocus"
  },
  // csv
  {
    "key": "ctrl+]",
    "command": "rainbow-csv.Align",
    "when": "editorLangId==csv"
  },
  {
    "key": "ctrl+shift+]",
    "command": "csv.preview",
    "when": "editorLangId==csv || editorLangId==tsv"
  },
  {
    "key": "ctrl+]",
    "command": "csv.clearState",
    "when": "!textInputFocus"
  },
  {
    "key": "ctrl+[",
    "command": "rainbow-csv.Shrink",
    "when": "editorLangId==csv"
  },
  // markdown
  //// mdプレビュー
  {
    "key": "ctrl+]",
    "command": "markdown.showPreview",
    "when": "editorLangId==markdown && !notebookEditorForcused"
  },
  //// mdプレビュー ( 右画面 )
  {
    "key": "ctrl+shift+]",
    "command": "markdown.showPreviewToSide",
    "when": "editorLangId==markdown && !notebookEditorForcused"
  }
]
